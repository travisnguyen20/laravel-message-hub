{
    "name": "travisnguyen20/laravel-message-hub",
    "description": "Message Hub For Laravel 5.*",
    "keywords": ["laravel", "framework", "message hub"],
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Nguyen",
            "email": "travisnguyen.me@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "TravisNguyen\\MessageHub\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "fzaninotto/faker": "^1.6",
        "mockery/mockery": "^0.9.5",
        "satooshi/php-coveralls": "^1.0",
        "friendsofphp/php-cs-fixer": "^1.12",
        "sllh/php-cs-fixer-styleci-bridge": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "TravisNguyen\\MessageHub\\MessageHubServiceProvider"
            ],
            "aliases": {
                "MessageHub": "TravisNguyen\\MessageHub\\Facades\\MessageHub"
            }
        }
    }
}
